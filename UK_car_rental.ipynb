    "uk_cars_mercedes[\"Make\"] = \"Mercedes\"\n",
    "uk_cars_skoda = pd.read_csv(\"skoda.csv\")\n",
    "uk_cars_skoda[\"Make\"] = \"Skoda\"\n",
    "uk_cars_toyota = pd.read_csv(\"toyota.csv\")\n",
    "uk_cars_toyota[\"Make\"] = \"Toyota\"\n",
